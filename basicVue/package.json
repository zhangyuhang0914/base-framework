{
  "name": "basicvueel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:prod": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "vue-tsc --noEmit",
    "sass:watch": "sass --watch src/styles:dist/styles",
    "sass:build": "sass src/styles:dist/styles --style=compressed",
    "pug:compile": "pug src/templates --out dist/templates",
    "style:check": "stylelint \"src/**/*.{css,scss,sass,vue}\"",
    "style:fix": "stylelint \"src/**/*.{css,scss,sass,vue}\" --fix"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "alova": "^3.3.4",
    "ant-design-vue": "^4.2.6",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "vue": "^3.5.17",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-legacy": "^7.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-pinia": "^0.4.1",
    "eslint-plugin-vue": "^10.4.0",
    "js-cookie": "^3.0.5",
    "postcss": "^8.5.6",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "pug": "^3.0.3",
    "sass": "^1.90.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pug": "^0.4.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  }
}
